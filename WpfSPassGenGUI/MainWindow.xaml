<Window x:Class="WpfSPassGenGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Password Generator (WPF-GUI)" Height="146.443" Width="322.436" Icon="Icon.ico" WindowStyle="ThreeDBorderWindow">
    <Grid HorizontalAlignment="Left" Height="390" Margin="24,20,0,0" VerticalAlignment="Top" Width="567">
        <TextBlock x:Name="passwordGenerated" Margin="0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="38" Width="547" FontSize="18">
            <TextBlock.Effect>
                <DropShadowEffect/>
            </TextBlock.Effect>
        </TextBlock>
        <Button x:Name="generateButton" Content="Generate" HorizontalAlignment="Left" Margin="157,43,0,0" VerticalAlignment="Top" Width="106" Height="37" Click="Button_Click"/>
        <Button x:Name="copyButton" Content="Copy to clipboard" HorizontalAlignment="Left" Margin="13,43,0,0" VerticalAlignment="Top" Width="129" Height="37" Click="copyButton_Click"/>
    </Grid>
</Window>
